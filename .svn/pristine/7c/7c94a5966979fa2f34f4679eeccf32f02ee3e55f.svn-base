<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ehome"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="17" />
 <!--   此为application下的第一行  启动报错 临时删除 android:name="com.ehome.BaiDuMap.DemoApplication"--> 
    <application
        android:name="com.ehome.SpeechApp"
        android:allowBackup="true"
        android:icon="@drawable/smart_home"
        android:label="@string/app_name"
        android:theme="@android:style/Theme.NoTitleBar" >
        <activity
            android:name="com.ehome.WelcomeActivity"
            android:label="@string/app_name"
             >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        
        <activity android:name="org.achartengine.GraphicalActivity"/>
        
        <activity				  
            android:name="com.ehome.LoginActivity"
            android:label="主页面" >
