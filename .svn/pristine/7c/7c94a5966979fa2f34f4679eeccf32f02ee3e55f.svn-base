        <!--  百度地图 Activity配置开始 -->
        
         <activity
            android:name="com.ehome.BaiDuMap.MapControlDemo"
            android:configChanges="orientation|keyboardHidden"
            android:label="地图操作功能"
            android:screenOrientation="sensor" >
        </activity>
                <service
            android:name="com.ehome.location.f"
            android:enabled="true"
            android:process=":remote" >
        </service>
         <activity
            android:name="com.ehome.BaiDuMap.RoutePlanDemo"
