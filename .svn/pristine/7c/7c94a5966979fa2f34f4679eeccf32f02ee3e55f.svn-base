            android:label="地图模式"
            android:screenOrientation="sensor" >
        </activity>
        
        <!--  百度地图 Activity配置结束 -->
        
        <!-- 服务推送配置开始 -->
         <!-- Polling Service -->
        <service android:name="com.ehome.tuisong.server.PollingService">
            <intent-filter>
                <action android:name="com.ehome.tuisong.server.PollingService"/>
            </intent-filter>
        </service>
        <!-- 服务推送配置结束 -->
        
        <!--  -->
        <activity android:name="com.ehome.cammonitor.ServerAct" android:screenOrientation="landscape">
    </activity>
